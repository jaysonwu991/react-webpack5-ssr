{
  "name": "react-webpack5-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "rm -rf dist && webpack --config config/webpack.client.dev.js --config config/webpack.server.dev.js && node dist/server.js",
    "startWatch": "npm-run-all --parallel startWatch:*",
    "startWatch:server": "nodemon --watch dist --exec node dist/server.js",
    "startWatch:build-client": "webpack --config config/webpack.client.dev.js --watch --no-cache",
    "startWatch:build-server": "webpack --config config/webpack.server.dev.js --watch",
    "build:dev": "rm -rf dist && webpack --config config/webpack.client.dev.js --config config/webpack.server.dev.js",
    "build:prod": "rm -rf dist && webpack --config config/webpack.client.prod.js config/webpack.server.prod.js"
  },
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "@loadable/server": "^5.15.3",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@loadable/babel-plugin": "^5.15.3",
    "@loadable/webpack-plugin": "^5.15.2",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-middleware": "^5.3.0",
    "@types/webpack-env": "^1.18.0",
    "@types/webpack-hot-middleware": "^2.25.6",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "sass-embedded": "^1.58.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
