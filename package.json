{
  "name": "react-webpack5-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf dist && webpack --config config/webpack.client.dev.js --config config/webpack.server.dev.js && run-p dev:**",
    "dev:build-server": "webpack --config config/webpack.server.dev.js --watch",
    "dev:build-client": "webpack --config config/webpack.client.dev.js --watch",
    "dev:watch-server": "nodemon --watch dist --exec node dist/server/server.js",
    "build": "rm -rf dist && webpack --config config/webpack.client.prod.js --config config/webpack.server.prod.js"
  },
  "dependencies": {
    "@loadable/component": "^5.16.7",
    "@loadable/server": "^5.16.7",
    "compression": "^1.8.1",
    "express": "^4.21.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serialize-javascript": "^6.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@loadable/babel-plugin": "^5.16.1",
    "@loadable/webpack-plugin": "^5.15.2",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.23",
    "@types/loadable__component": "^5.13.10",
    "@types/loadable__server": "^5.12.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/serialize-javascript": "^5.0.4",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.8",
    "@types/webpack-hot-middleware": "^2.25.10",
    "babel-loader": "^9.2.1",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "mini-css-extract-plugin": "^2.9.4",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "sass-embedded": "^1.90.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.3",
    "webpack-dev-server": "^4.15.2",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-node-externals": "^3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
